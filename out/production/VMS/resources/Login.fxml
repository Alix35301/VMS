      <ChoiceBox fx:id="choiceBoxLogin" layoutX="523.0" layoutY="310.0" prefHeight="28.0" prefWidth="152.0" />
      <Button layoutX="540.0" layoutY="351.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button layoutX="604.0" layoutY="351.0" mnemonicParsing="false" text="Reset" />
      <Separator layoutX="402.0" layoutY="-6.0" orientation="VERTICAL" prefHeight="631.0" prefWidth="23.0" />
      <Label fx:id="lbllogout" layoutX="565.0" layoutY="406.0" text="Logged Out" />
   </children>
