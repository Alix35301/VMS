<AnchorPane fx:id="rootPane" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label layoutX="675.0" layoutY="178.0" text="VMS Login">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="675.0" layoutY="238.0" promptText="Username/email" />
      <PasswordField fx:id="txtPassword" layoutX="675.0" layoutY="277.0" onAction="#login" promptText="Password" />
      <ChoiceBox fx:id="choiceBoxLogin" layoutX="675.0" layoutY="316.0" prefHeight="28.0" prefWidth="152.0" />
      <Button layoutX="692.0" layoutY="357.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button layoutX="756.0" layoutY="357.0" mnemonicParsing="false" text="Reset" />
      <Separator layoutX="489.0" layoutY="65.0" orientation="VERTICAL" prefHeight="631.0" prefWidth="23.0" />
      <Label fx:id="lbllogout" layoutX="717.0" layoutY="412.0" text="Logged Out" />
   </children>
</AnchorPane>
