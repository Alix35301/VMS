<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SearchTwitterController">
   <children>
      <TextField fx:id="searchTxtF" layoutX="263.0" layoutY="145.0" prefHeight="40.0" prefWidth="300.0" promptText="Please enter the keyword">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button layoutX="655.0" layoutY="145.0" mnemonicParsing="false" onAction="#search" prefHeight="40.0" prefWidth="129.0" text="Search" />
      <TableView fx:id="resultsTable" layoutX="31.0" layoutY="221.0" prefHeight="453.0" prefWidth="938.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ChoiceBox fx:id="viewCountCb" layoutX="572.0" layoutY="145.0" prefHeight="40.0" prefWidth="79.0" />
      <Button fx:id="back" layoutX="56.0" layoutY="37.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="logout" layoutX="887.0" layoutY="37.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Label layoutX="383.0" layoutY="38.0" text="Twitter Market Analysis" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
